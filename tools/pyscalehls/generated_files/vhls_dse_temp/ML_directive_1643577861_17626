set_directive_array_partition -type complete -dim 1 kernel_2mm v2
set_directive_array_partition -type complete -dim 2 kernel_2mm v2
set_directive_array_partition -type complete -dim 1 kernel_2mm v3

set_directive_array_partition -type block -factor 4 -dim 1 kernel_2mm v4
set_directive_array_partition -type block -factor 16 -dim 2 kernel_2mm v4
set_directive_array_partition -type cyclic -factor 64 -dim 1 kernel_2mm v5

set_directive_array_partition -type complete -dim 1 kernel_2mm v6
set_directive_array_partition -type complete -dim 2 kernel_2mm v6