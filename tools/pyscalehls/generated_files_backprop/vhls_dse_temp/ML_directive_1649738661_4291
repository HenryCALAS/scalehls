set_directive_array_partition -type block -factor 8 -dim 1 backprop weights1
set_directive_array_partition -type cyclic -factor 64 -dim 1 backprop weights2
set_directive_array_partition -type block -factor 6 -dim 1 backprop weights3
set_directive_array_partition -type complete -dim 1 backprop biases1
set_directive_array_partition -type complete -dim 1 backprop biases2



set_directive_array_partition -type complete -dim 1 backprop activations1

set_directive_array_partition -type block -factor 3 -dim 1 backprop activations3
set_directive_array_partition -type cyclic -factor 64 -dim 1 backprop dactivations1
set_directive_array_partition -type block -factor 64 -dim 1 backprop dactivations2




set_directive_array_partition -type block -factor 64 -dim 1 backprop delta_weights2

set_directive_array_partition -type complete -dim 1 backprop oracle_activations1
set_directive_array_partition -type cyclic -factor 8 -dim 1 backprop oracle_activations2